<div class="container" role="main">
  <h1>QC Logs  &nbsp; </h1>
  <div class="table-responsive">
    <table class="table table-striped table-middle-align">
      <thead>
        <tr>
          <th>Date</th>
          <th>Owner</th>
          <th>Applicator</th>
          <th>Created By</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @qc_logs.each do |log| %>
          <tr><% log.attributes.each do |key, value| %>
                <td><%= value %></td><% end %></tr>
        <% end %>
      </tbody>
    </table>
  </div>

</div>

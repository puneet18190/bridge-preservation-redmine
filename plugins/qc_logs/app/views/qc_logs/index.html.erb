<div class="container" role="main">
  <h1>QC Logs  &nbsp; </h1>
  <div class="table-responsive">
    <table class="table table-striped table-middle-align">
      <thead>
        <tr>
          <th>Date</th>
          <th>Applicator</th>
          <th>Created By</th>
          <th>Status</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @qc_logs.each do |log| %>
          <tr>
            <td>
              <%= log["date"] %>
            </td>
            <td>
              <%= log["approved_applicator"] %>
            </td>
            <td>
              <%= User.find(log["user_id"]).name %>
            </td>
            <td>
              <%= log["status"] %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>

</div>
